{
  "name": "Trello | Custom fields first",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Pushes the custom fields above the description",
  "homepage_url": "https://codebrauer.com",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Trello | Custom fields first"
  },
  "permissions": [
    "activeTab",
    "https://trello.com/*",
    "tabs"
  ]
}